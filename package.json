{
  "name": "isomorphic-react-starter",
  "version": "0.0.1",
  "description": "A starter kit using React on both the server and client",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "compile": "babel -d server/ src/components/**/*.js src/entry-server.js",
    "build": "parcel build src/entry-client.js --out-file main.js --out-dir dist",
    "dev": "NODE_ENV=development node index.js",
    "start": "npm run compile && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CAYdenberg/isomorphic-react-starter.git"
  },
  "author": "@CAYdenberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CAYdenberg/isomorphic-react-starter/issues"
  },
  "homepage": "https://github.com/CAYdenberg/isomorphic-react-starter#readme",
  "dependencies": {
    "bulma": "^0.7.2",
    "email-validator": "^2.0.4",
    "express": "^4.16.4",
    "express-hbs": "^2.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-standard": "^4.0.0",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.15.3"
  }
}
